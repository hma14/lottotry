        // Set default tab for TabbedPanels
        function setDefaultTab(tab) {
            var TabbedPanels1 = new Spry.Widget.TabbedPanels("TabbedPanels1", { defaultTab: tab });

            TabbedPanels1['defaultTab'].value = tab;
            TabbedPanels1.showPanel(tab);

