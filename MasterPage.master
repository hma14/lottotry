        function InitMenu() {
            $('.menu li').hover(function () {
                $('ul:first', this).css({ visibility: "visible", display: "none" }).fadeIn(200).show();
            }, function () {
                $('ul:first', this).css({ visibility: "hidden" });
            });
        }

